{
  "name": "guestlist",
  "version": "2.2.0",
  "description": "Guestlist is a tiny library that whitelists, validates, and sanitizes HTTP request properties",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "dependencies": {
    "@types/validator": "^12.0.0",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "coveralls": "^3.0.2",
    "express": "^4.17.0",
    "jasmine": "^3.4.0",
    "node-mocks-http": "^1.7.5",
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "rollup": "^2.11.0"
  },
  "scripts": {
    "coverage": "nyc npm test",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "npm run lint && npm run spec",
    "spec": "jasmine spec/*.spec.js",
    "lint": "prettier --config .prettierrc.json --list-different src/*.js",
    "build": "rollup -c rollup.config.js"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "keywords": [
    "koa",
    "express",
    "fastify",
    "middleware",
    "validation",
    "sanitization",
    "querystring",
    "formdata"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:i-like-robots/guestlist.git"
  },
  "homepage": "https://github.com/i-like-robots/guestlist",
  "author": "Matt Hinchliffe",
  "license": "MIT"
}
