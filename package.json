{
  "name": "guestlist",
  "version": "0.0.1",
  "description": "Whitelists, validates and sanitizes all of your request parameters.",
  "main": "dist-cjs/index.js",
  "module": "dist-es6/index.js",
  "types": "typings/index.d.ts",
  "dependencies": {
    "validator": "^9.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/jasmine": "^2.8.6",
    "@types/validator": "^6.3.0",
    "coveralls": "^3.0.0",
    "express": "^4.16.2",
    "jasmine": "^2.9.0",
    "nyc": "^11.4.1",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.0"
  },
  "scripts": {
    "coverage": "nyc npm test",
    "pretest": "npm run lint && npm run build:cjs",
    "test": "jasmine spec/*.spec.js",
    "lint": "tslint -c tslint.json src/**/*.ts spec/*.js",
    "build": "npm run build:cjs && npm run build:es6",
    "build:cjs": "tsc --outDir dist-cjs --module commonjs",
    "build:es6": "tsc --outDir dist-es6 --module es2015",
    "prepublish": "npm run build"
  },
  "files": [
    "dist-cjs",
    "dist-es6",
    "typings",
    "examples",
    "README.md"
  ],
  "engines": {
    "node": ">= 6.0.0"
  },
  "keywords": [
    "express",
    "middleware",
    "validation",
    "querystring",
    "formdata"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:i-like-robots/guestlist.git"
  },
  "homepage": "https://github.com/i-like-robots/guestlist",
  "author": "Matt Hinchliffe",
  "license": "ISC"
}
